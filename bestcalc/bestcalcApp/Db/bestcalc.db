record(acalcout, "$(P):BPM0:PosX_Mean")
{
        field(DESC, "Waveform Position X Mean")
        field(INAA, "$(P):BPM0:PosX CP")
        field(NELM, 1024)
        field(CALC, "AVG(AA)")
        field(PINI, 1)
        field(EGU, "um")
        field(PREC, 5)
}

