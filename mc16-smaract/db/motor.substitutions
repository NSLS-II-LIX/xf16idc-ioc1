{"XF:16IDC-ES", ":Scan2-Gonio{Ax:tZ}Mtr",   "motor", "P0", 0,    "ss_tZ",   asynMotor, "deg", 1.719e-6, 2    }
{"XF:16IDC-ES", ":Scan2-Gonio{Ax:tX}Mtr",   "motor", "P0", 1,    "ss_tX",   asynMotor, "deg", 1.719e-6, 2    }
}

