file "db/motor.db"
{

# ADDR ---> axis #
# NOTE ---> 2 asyn ports: 1) lowel comms ("P0"), and 2) Motor Record ("motor")
pattern
{ P,            M,                   MOTOR,   PORT, ADDR, DESC,           DTYP,      EGU,    MRES,   PREC }
{"XF:16IDB-OP", "{Slt:SSA1-Ax:dX}Mtr",   "motor", "P0", 0,    "SmarAct SSA1 dX",   asynMotor, "um", "1e-9", 2    }
{"XF:16IDB-OP", "{Slt:SSA1-Ax:dY}Mtr",   "motor", "P0", 1,    "SmarAct SSA1 dY",   asynMotor, "um", "1e-9", 2    }
}

