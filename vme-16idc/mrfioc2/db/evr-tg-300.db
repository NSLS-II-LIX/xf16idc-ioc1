#    Choices are: "Trig", "Set", "Reset"
#  EVT = Initial event code
#

record(longout, "$(SYS,undefined){$(D,undefined)-DlyGen:9}Evt:Trig2-SP") {
  field( DTYP, "EVR Pulser Mapping" )
  field( OUT , "@OBJ=$(EVR,undefined):Pul9, Func=Trig")
  field( PINI, "YES")
  field( DESC, "Mapping for Pulser 9")
  field( VAL , "0")
  field( LOPR, "0")
  field( HOPR, "255")
  field( DRVL, "0")
  field( DRVH, "255")
  info(autosaveFields_pass0, "VAL")
}
