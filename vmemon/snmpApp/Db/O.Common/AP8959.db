record(stringout, "$(SYS,undefined)$(DEV,undefined)Sw:Name17-SP")
{
	field(DESC, "Set Outlet17 Name")
	field(DTYP, "Snmp")
	field(OUT,  "@$(IP,undefined) private PowerNet-MIB::sPDUOutletName.17")
	field(SCAN, "Passive")
	field(FLNK, "$(SYS,undefined)$(DEV,undefined)Sw:Name17-RB")
	field(VAL,  "Outlet 17")
	info(autosaveFields, "VAL")
	field(PINI, "1")
}

