# Database for APC PDU

record(ai,"$(SYS,undefined)$(DEV,undefined)I-I_")
{
	field(DTYP,"Snmp")
	field(INP ,"@$(IP,undefined) private PowerNet-MIB::rPDULoadStatusLoad.1")
	field(SCAN,"1 second")
	field(FLNK,"$(SYS,undefined)$(DEV,undefined)I-I")
}

record(calc,"$(SYS,undefined)$(DEV,undefined)I-I")
{
	field(INPA, "$(SYS,undefined)$(DEV,undefined)I-I_")
	field(DESC, "Load (Current)")
	field(CALC, "A*0.1")
	field(EGU , "Amps")
	field(HIGH, "12")
	field(HIHI, "16")
	field(HHSV, "MAJOR")
	field(HSV,  "MINOR")
	field(HOPR, "20")
	field(LOPR, "0")
}

record(longin, "$(SYS,undefined)$(DEV,undefined)I:Max-RB")
{
	field(DTYP,"Snmp")
        field(INP ,"@$(IP,undefined) private .1.3.6.1.4.1.318.1.1.12.2.2.1.1.4.1") #PowerNet-MIB::rPDULoadPhaseConfigOverloadThreshold.phase1") 
        field(SCAN,"5 second")
}

record(longout, "$(SYS,undefined)$(DEV,undefined)I:Max-SP")
{
        field(DTYP,"Snmp")
        field(OUT ,"@$(IP,undefined) private .1.3.6.1.4.1.318.1.1.12.2.2.1.1.4.1")
	field(VAL ,"16")
	field(SCAN, "Passive")
	field(PINI,"1")
	field(FLNK,"$(SYS,undefined)$(DEV,undefined)I:Max-RB")
	info(autosaveFields, "VAL")
}

record(longin, "$(SYS,undefined)$(DEV,undefined)I:Warn-RB")
{
        field(DTYP,"Snmp")
        field(INP ,"@$(IP,undefined) private .1.3.6.1.4.1.318.1.1.12.2.2.1.1.3.1") # PowerNet-MIB::rPDULoadPhaseConfigNearOverloadThreshold.phase1
        field(SCAN,"5 second")
}

record(longout, "$(SYS,undefined)$(DEV,undefined)I:Warn-SP")
{
        field(DTYP,"Snmp")
        field(OUT ,"@$(IP,undefined) private .1.3.6.1.4.1.318.1.1.12.2.2.1.1.3.1")
	field(VAL ,"12")
	field(PINI,"1")
	field(FLNK,"$(SYS,undefined)$(DEV,undefined)I:Warn-RB")
	info(autosaveFields, "VAL")
}       

record(longin, "$(SYS,undefined)$(DEV,undefined)Pwr-Sts_")
{
	field(DTYP,"Snmp")
	field(INP ,"@$(IP,undefined) private PowerNet-MIB::rPDULoadStatusLoadState.1")
	field(SCAN,"1 second")
	field(FLNK,"$(SYS,undefined)$(DEV,undefined)Pwr-Sts")
}

record(mbbi,"$(SYS,undefined)$(DEV,undefined)Pwr-Sts")
{
	field(DESC,"Load Status")
	field(INP, "$(SYS,undefined)$(DEV,undefined)Pwr-Sts_")
	field(ONST,"Normal")
	field(TWST,"Low")
	field(THST,"NearOverload")
	field(FRST,"Overload")
	field(ONSV,"NO_ALARM")
	field(TWSV,"NO_ALARM")
	field(THSV,"MINOR")
	field(FRST,"MAJOR")
}

record(ai,"$(SYS,undefined)$(DEV,undefined)Pwr-I")
{
    	field(DESC,"Load (Power)")
    	field(DTYP,"Snmp")
    	field(INP,"@$(IP,undefined) private PowerNet-MIB::rPDUIdentDevicePowerWatts.0")
    	field(SCAN,"1 second")
	field(EGU, "Watts")
	field(LOPR,"0")
	field(HOPR,"4200")
	field(HIGH,"2496")
	field(HIHI,"3328")
	field(HHSV, "MAJOR")
        field(HSV,  "MINOR")
}

