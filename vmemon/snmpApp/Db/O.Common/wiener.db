# Main switch
record (bo, "$(SYS,undefined){$(D,undefined)-System}MainSwitch-Sel") {
        field(DESC, "Main Switch Set")
        field( ZNAM, "Off")
        field( ONAM, "On")
        field( OUT, "$(SYS,undefined){$(D,undefined)-System}MainSwitch-SP_ PP")
}

record (longout,"$(SYS,undefined){$(D,undefined)-System}MainSwitch-SP_")
{
	field (DESC, "Main Switch RSet")
	field (DTYP, "Snmp")
        field (OUT,  "@$(IP,recursive) admin INTEGER: 128 i")
	field (HOPR, "1")
	field (LOPR, "0")
        info (OID, "WIENER-CRATE-MIB::sysMainSwitch.0")
}

record (longin,"$(SYS,undefined){$(D,undefined)-System}MainSwitch-RB_")
{
	field (DESC, "Main Switch RValue")
	field (DTYP, "Snmp")
        field (INP,  "@$(IP,recursive) admin ( 128")
        field (SCAN,"5 second")
        field (FLNK, "$(SYS,undefined){$(D,undefined)-System}MainSwitch-RB")
        info (OID, "WIENER-CRATE-MIB::sysMainSwitch.0")
}

record (bi, "$(SYS,undefined){$(D,undefined)-System}MainSwitch-RB") {
        field (DESC, "Main Switch Value")
        field (INP,  "$(SYS,undefined){$(D,undefined)-System}MainSwitch-RB_")
        field (ZNAM, "Off")
        field (ONAM, "On")
}

