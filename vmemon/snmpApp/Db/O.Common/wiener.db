record (calc,"$(SYS,undefined){$(D,undefined)-Sensor}Temp:MAX-I")
{
	field (DESC, "Calculate Maximum Temperature")
	field (EGU,  "C")
	field (SCAN, "5 second")
	field (INPA, "$(SYS,undefined){$(D,undefined)-Sensor}Temp:1-I")
	field (INPB, "$(SYS,undefined){$(D,undefined)-Sensor}Temp:2-I")
	field (INPC, "$(SYS,undefined){$(D,undefined)-Sensor}Temp:3-I")
	field (INPD, "$(SYS,undefined){$(D,undefined)-Sensor}Temp:4-I")
	field (INPE, "$(SYS,undefined){$(D,undefined)-Sensor}Temp:5-I")
	field (INPF, "$(SYS,undefined){$(D,undefined)-Sensor}Temp:6-I")
	field (INPG, "$(SYS,undefined){$(D,undefined)-Sensor}Temp:7-I")
	field (INPH, "$(SYS,undefined){$(D,undefined)-Sensor}Temp:8-I")
	field (CALC, "MAX(A,B,C,D,E,F,G,H)")
	field (ADEL, "1")
	field (HIGH, "40")
	field (HIHI, "50")
	field (HSV,  "MINOR")
	field (HHSV, "MAJOR")
}

