# Limits for -12V are reported as positive numbers :(
# so we play games to set them correctly
record(calcout, "$(SYS,undefined){$(D,undefined)-Output}MinMax:1-FOut_")
{
	field(INPA, "$(SYS,undefined){$(D,undefined)-Output}SMinV:1-RB NPP")
	field(INPB, "$(SYS,undefined){$(D,undefined)-Output}SMaxV:1-RB NPP")
	field(INPC, "1")
	field(CALC, "C>0?MIN(A,B):MIN(-A,-B)")
	field(OUT,  "$(SYS,undefined){$(D,undefined)-Output}MV:1-I.LOW")
	field(FLNK, "$(SYS,undefined){$(D,undefined)-Output}MinMax2:1-FOut_")
}

